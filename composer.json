{
  "name": "simple-shop/repositories",
  "description": "laravel商品系统",
  "license": "MIT",
  "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
  "authors": [
    {
      "name": "Wangzd",
      "email": "wangzhoudong@foxmail.com"
    },
    {
      "name": "jiangzhiheng",
      "email": "j5110021@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^5.2",
    "illuminate/database": "^5.2",
    "illuminate/filesystem": "^5.2",
    "illuminate/console": "^5.2"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "SimpleShop": "src/SimpleShop"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SimpleShop\\Repositories\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
